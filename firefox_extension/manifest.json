{
    "manifest_version": 2,
    "name": "Ask a question",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "contextMenus",
      "storage",
      "tabs"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  